{
  "name": "action-gitflow-setup",
  "version": "2.0.2",
  "description": "A GitHub Action helper to determine environment to build/deploy actions",
  "main": "lib/main.js",
  "repository": "git@github.com:Zajno/action-gitflow-setup.git",
  "author": "Zajno <hello@zajno.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "test": "act",
    "test:pull": "yarn test pull_request -e src/pull_request.json",
    "test:push": "yarn test push",
    "package": "ncc build src/main.ts",
    "all": "npm run build && npm run package"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.4",
    "@vercel/ncc": "^0.38.0",
    "typescript": "^5.1.6"
  }
}
